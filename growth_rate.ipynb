   "source": [
    "import numpy as np\n",
    "from matplotlib import pyplot as plt\n",
    "\n",
    "dx=[5]\n",
    "GR=1.2\n",
    "for i in range(10):\n",
    "    dx.append(dx[-1]*GR)\n",
    "x_vals = np.cumsum(dx)\n",
    "plt.scatter(x_vals, dx)\n",
    "plt.scatter(x_vals, x_vals*(GR-1))"
   ],
